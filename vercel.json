{
  "version": 2,
  "buildCommand": "npm install && npx vite build",
  "outputDirectory": "dist/public", 
  "installCommand": "npm install",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}